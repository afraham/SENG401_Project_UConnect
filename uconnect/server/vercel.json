{
	"version": 2,
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"rewrites": [{ "source": "/(.*)", "destination": "src/app.js" }],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
		},
		{
			"source": "/https://u-connect-server.vercel.app/(.*)",
			"headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
		}
	]
}
